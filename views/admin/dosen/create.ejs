<%- include ../../partials/header.ejs %>
<%- include ../../partials/navbar.ejs %>
<%- include ../../partials/sidebar.ejs %>

<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1>Dosen Pembimbing</h1>
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="/">Beranda</a></li>
              <li class="breadcrumb-item"><a href="/dosen">Dosen Pembimbing</a></li>
              <li class="breadcrumb-item active">Tambah</li>
            </ol>
          </div>
        </div>
      </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="container-fluid">
        <div class="row">
          <div class="col-12">
            <div class="card card-primary">
                <div class="card-header">
                  <h3 class="card-title">Form Tambah Dosen Pembimbing</h3>
                </div>
                <!-- /.card-header -->
                <!-- form start -->
                <form action="/dosen/create" method="post">
                  <div class="card-body">
                    <div class="form-group">
                      <label for="nidn">No Induk Dosen Nasional <sup style="color: #ff0000;">*</sup></label>
                      <input type="text" class="form-control" id="nidn" name="nidn"  placeholder="Masukan NIDN dosen" required>
                    </div>

                    <div class="form-group">
                      <label for="nama">Nama Lengkap <sup style="color: #ff0000;">*</sup></label>
                      <input type="text" class="form-control" id="nama" name="nama"  placeholder="Masukan nama lengkap dosen" required>
                    </div>
                    
                    <div class="form-group">
                      <label for="gelar">Gelar <sup style="color: #ff0000;">*</sup></label>
                      <input type="text" class="form-control" id="gelar" name="gelar" placeholder="Masukan gelar dosen" required>
                    </div>
                      
                    <div class="form-group">
                      <label for="email">Email <sup style="color: #ff0000;">*</sup></label>
                      <input type="email" class="form-control" id="email" name="email" placeholder="Masukan email dosen" required>
                    </div>

                    <div class="form-group">
                      <label for="no_wa">No Whatsapp <sup style="color: #ff0000;">*</sup></label>
                      
                      <div class="input-group-prepend">
                        <span class="input-group-text">+62</span>
                        <input type="number" class="form-control" id="no_wa" name="no_wa" placeholder="Masukan no whatsapp dosen" required>
                      </div>
                    </div>

                    <div class="form-group">
                        <label for="pendidikan">Jenjang Pendidikan <sup style="color: #ff0000;">*</sup></label>
                        <select id="pendidikan" name="pendidikan" class="form-control select2" style="width: 100%;" required>
                            <option selected="selected" value="" hidden>Masukan jenjang pendidikan dosen</option>
                            <option value="0.5">S2</option>
                            <option value="1">S3</option>
                        </select>
                    </div>
                      
                    <div class="form-group">
                      <label for="fungsional">Jabatan Fungsional <sup style="color: #ff0000;">*</sup></label>
                      <select id="fungsional" name="fungsional" class="form-control select2" style="width: 100%;" required>
                          <option selected="selected" value="" hidden>Masukan jabatan fungsional dosen</option>
                          <option value="0">Belum Ada Jabatan Fungsional</option>
                          <option value="0.25">Tenaga Pengajar</option>
                          <option value="0.5">Asisten Ahli</option>
                          <option value="0.75">Lektor</option>
                          <option value="1">Lektor Kepala</option>
                      </select>
                    </div>
                    
                    <div class="form-group">
                      <label for="jumlah">Jumlah Bimbingan <sup style="color: #ff0000;">*</sup></label>
                      <select id="jumlah" name="jumlah" class="form-control select2" style="width: 100%;" required>
                          <option selected="selected" value="" hidden>Masukan jumlah bimbingan dosen</option>
                          <option value="0">21 - 25 (Sangat Banyak)</option>
                          <option value="0.25">16 - 20 (Banyak)</option>
                          <option value="0.5">11 - 15 (Cukup)</option>
                          <option value="0.75">06 - 10 (Sedikit)</option>
                          <option value="1">01 - 05 (Sangat Sedikit)</option>
                      </select>
                    </div>

                    <br/>
                    <h4><b> Bidang Keahlian </b></h4>
                    <span style="font-size: 14px;">
                      Diisi berdasarkan tingkat keahlian dari tingkat 1 sampai tingkat 8.
                      Tingkat 1 merupakan tingkat yang sangat ahli, sedangkan tingkat 8 merupakan yang paling tidak dikuasai
                    </span>
                    <b> <hr/> </b>
                    <br/>
                    
                    <div class="form-group">
                      <label for="bidang_aplikasi_desktop"> Aplikasi Desktop <sup style="color: #ff0000;">*</sup></label>
                      <select id="bidang_aplikasi_desktop" name="bidang_aplikasi_desktop" class="form-control select2" style="width: 100%;" required>
                          <option selected="selected" value="" hidden>Masukan tingkat keahlian dosen, pada bidang aplikasi desktop</option>
                          <option value="0.125">Tingkat 8</option>
                          <option value="0.250">Tingkat 7</option>
                          <option value="0.375">Tingkat 6</option>
                          <option value="0.500">Tingkat 5</option>
                          <option value="0.625">Tingkat 4</option>
                          <option value="0.750">Tingkat 3</option>
                          <option value="0.875">Tingkat 2</option>
                          <option value="1">Tingkat 1</option>
                      </select>
                    </div>

                    <div class="form-group">
                      <label for="bidang_aplikasi_web"> Aplikasi Web <sup style="color: #ff0000;">*</sup></label>
                      <select id="bidang_aplikasi_web" name="bidang_aplikasi_web" class="form-control select2" style="width: 100%;" required>
                          <option selected="selected" value="" hidden>Masukan tingkat keahlian dosen, pada bidang aplikasi web</option>
                          <option value="0.125">Tingkat 8</option>
                          <option value="0.250">Tingkat 7</option>
                          <option value="0.375">Tingkat 6</option>
                          <option value="0.500">Tingkat 5</option>
                          <option value="0.625">Tingkat 4</option>
                          <option value="0.750">Tingkat 3</option>
                          <option value="0.875">Tingkat 2</option>
                          <option value="1">Tingkat 1</option>
                      </select>
                    </div>
                    
                    <div class="form-group">
                      <label for="bidang_aplikasi_mobile"> Aplikasi Mobile <sup style="color: #ff0000;">*</sup></label>
                      <select id="bidang_aplikasi_mobile" name="bidang_aplikasi_mobile" class="form-control select2" style="width: 100%;" required>
                          <option selected="selected" value="" hidden>Masukan tingkat keahlian dosen, pada bidang aplikasi mobile</option>
                          <option value="0.125">Tingkat 8</option>
                          <option value="0.250">Tingkat 7</option>
                          <option value="0.375">Tingkat 6</option>
                          <option value="0.500">Tingkat 5</option>
                          <option value="0.625">Tingkat 4</option>
                          <option value="0.750">Tingkat 3</option>
                          <option value="0.875">Tingkat 2</option>
                          <option value="1">Tingkat 1</option>
                      </select>
                    </div>
                    
                    <div class="form-group">
                      <label for="bidang_data_mining"> Data Mining <sup style="color: #ff0000;">*</sup></label>
                      <select id="bidang_data_mining" name="bidang_data_mining" class="form-control select2" style="width: 100%;" required>
                          <option selected="selected" value="" hidden>Masukan tingkat keahlian dosen, pada bidang data mining</option>
                          <option value="0.125">Tingkat 8</option>
                          <option value="0.250">Tingkat 7</option>
                          <option value="0.375">Tingkat 6</option>
                          <option value="0.500">Tingkat 5</option>
                          <option value="0.625">Tingkat 4</option>
                          <option value="0.750">Tingkat 3</option>
                          <option value="0.875">Tingkat 2</option>
                          <option value="1">Tingkat 1</option>
                      </select>
                    </div>
                    
                    <div class="form-group">
                      <label for="bidang_iot"> Internet Of Thinks <sup style="color: #ff0000;">*</sup></label>
                      <select id="bidang_iot" name="bidang_iot" class="form-control select2" style="width: 100%;" required>
                          <option selected="selected" value="" hidden>Masukan tingkat keahlian dosen, pada bidang internet of thinks</option>
                          <option value="0.125">Tingkat 8</option>
                          <option value="0.250">Tingkat 7</option>
                          <option value="0.375">Tingkat 6</option>
                          <option value="0.500">Tingkat 5</option>
                          <option value="0.625">Tingkat 4</option>
                          <option value="0.750">Tingkat 3</option>
                          <option value="0.875">Tingkat 2</option>
                          <option value="1">Tingkat 1</option>
                      </select>
                    </div>
                    
                    <div class="form-group">
                      <label for="bidang_desain_aplikasi"> Desain Aplikasi <sup style="color: #ff0000;">*</sup></label>
                      <select id="bidang_desain_aplikasi" name="bidang_desain_aplikasi" class="form-control select2" style="width: 100%;" required>
                          <option selected="selected" value="" hidden>Masukan tingkat keahlian dosen, pada bidang desain aplikasi</option>
                          <option value="0.125">Tingkat 8</option>
                          <option value="0.250">Tingkat 7</option>
                          <option value="0.375">Tingkat 6</option>
                          <option value="0.500">Tingkat 5</option>
                          <option value="0.625">Tingkat 4</option>
                          <option value="0.750">Tingkat 3</option>
                          <option value="0.875">Tingkat 2</option>
                          <option value="1">Tingkat 1</option>
                      </select>
                    </div>
                    
                    <div class="form-group">
                      <label for="bidang_jaringan_komputer"> Jaringan Komputer <sup style="color: #ff0000;">*</sup></label>
                      <select id="bidang_jaringan_komputer" name="bidang_jaringan_komputer" class="form-control select2" style="width: 100%;" required>
                          <option selected="selected" value="" hidden>Masukan tingkat keahlian dosen, pada bidang jaringan komputer</option>
                          <option value="0.125">Tingkat 8</option>
                          <option value="0.250">Tingkat 7</option>
                          <option value="0.375">Tingkat 6</option>
                          <option value="0.500">Tingkat 5</option>
                          <option value="0.625">Tingkat 4</option>
                          <option value="0.750">Tingkat 3</option>
                          <option value="0.875">Tingkat 2</option>
                          <option value="1">Tingkat 1</option>
                      </select>
                    </div>
                    
                    <div class="form-group">
                      <label for="bidang_basis_data"> Basis Data <sup style="color: #ff0000;">*</sup></label>
                      <select id="bidang_basis_data" name="bidang_basis_data" class="form-control select2" style="width: 100%;" required>
                          <option selected="selected" value="" hidden>Masukan tingkat keahlian dosen, pada bidang basis data</option>
                          <option value="0.125">Tingkat 8</option>
                          <option value="0.250">Tingkat 7</option>
                          <option value="0.375">Tingkat 6</option>
                          <option value="0.500">Tingkat 5</option>
                          <option value="0.625">Tingkat 4</option>
                          <option value="0.750">Tingkat 3</option>
                          <option value="0.875">Tingkat 2</option>
                          <option value="1">Tingkat 1</option>
                      </select>
                    </div>
                  <!-- /.card-body -->
  
                  <div class="card-footer">
                    <a href="/dosen" class="btn btn-default">
                      <i class="fas fa-arrow-left"></i>
                      Kembali
                    </a>
                    <button type="submit" class="btn btn-primary float-right">
                      <i class="fas fa-save"></i>
                      Simpan
                    </button>
                  </div>
                </form>
              </div>
          </div>
          <!-- /.col -->
        </div>
        <!-- /.row -->
      </div>
      <!-- /.container-fluid -->
    </section>
    <!-- /.content -->
</div>

<%- include ../../partials/footer.ejs %>
<%- include ../../partials/js.ejs %>